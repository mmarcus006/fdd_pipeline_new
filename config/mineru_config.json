{
  "models-dir": "~/.mineru/models",
  "device-mode": "auto",
  "performance": {
    "batch-size": 2,
    "max-pages": 500,
    "enable-cache": true,
    "cache-dir": "~/.mineru/cache"
  },
  "layout-config": {
    "model": "doclayout_yolo",
    "confidence_threshold": 0.5,
    "nms_threshold": 0.45,
    "use_fp16": true
  },
  "formula-config": {
    "mfd_model": "yolo_v8_mfd",
    "mfr_model": "unimernet_small",
    "enable": true,
    "batch_size": 16,
    "use_fp16": true
  },
  "table-config": {
    "model": "rapid_table",
    "sub_model": "slanet_plus",
    "enable": true,
    "max_time": 400,
    "use_fp16": true
  },
  "ocr-config": {
    "lang": "en",
    "det_model": "en_PP-OCRv4_det",
    "rec_model": "en_PP-OCRv4_rec",
    "enable": true,
    "use_angle_cls": true,
    "drop_score": 0.5
  },
  "optimization": {
    "gpu_memory_fraction": 0.8,
    "enable_tensorrt": false,
    "enable_quantization": false,
    "num_workers": 4,
    "prefetch_factor": 2
  },
  "fallback": {
    "cpu_batch_size": 1,
    "cpu_num_threads": 8,
    "enable_cpu_fallback": true,
    "memory_limit_gb": 12
  }
}